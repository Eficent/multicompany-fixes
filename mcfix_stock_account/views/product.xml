<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_category_property_form" model="ir.ui.view">
            <field name="name">product.category.stock.property.form.inherit</field>
            <field name="model">product.category</field>
            <field name="inherit_id" ref="account.view_category_property_form"/>
            <field name="arch" type="xml">
                <field name="property_stock_account_input_categ_id" position="replace">
                    <field name="property_stock_account_input_categ_id"
                           domain="[('deprecated', '=', False), ('company_id','=',current_company_id)]"/>
                </field>
                <field name="property_stock_account_output_categ_id" position="replace">
                    <field name="property_stock_account_output_categ_id"
                           domain="[('deprecated', '=', False), ('company_id','=',current_company_id)]"/>
                </field>
                <field name="property_stock_valuation_account_id" position="replace">
                    <field name="property_stock_valuation_account_id"
                           domain="[('deprecated', '=', False), ('company_id','=',current_company_id)]"/>
                </field>
                <field name="property_stock_journal" position="replace">
                    <field name="property_stock_journal"
                           domain="[('company_id','=',current_company_id)]"/>
                </field>
            </field>
        </record>
    </data>
</odoo>